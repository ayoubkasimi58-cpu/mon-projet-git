function  thisisme(){
    let first = ["C1", "C3","C2", "C1" , "C8" , "C2" ,"C1" , "C4"];
    let two = prompt("ecris le code Ã  chercer(ex: C1,C2,C3...):")
    let  three= 0;
    for ( let i= 0; i < first.length; i++){
        if (first[i] === two){
            three++;
        }
    }
    console.log( " la code " + two + " apparait " + three + " fois. " ) 
}
thisisme(); 
